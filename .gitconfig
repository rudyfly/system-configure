[core]
	editor = vim
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[color]
	ui = true
[alias]
	co = checkout
	br = branch
	cm = commit
	st = status
	df = diff
	lg = log
	lo = log --oneline
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !gitk
